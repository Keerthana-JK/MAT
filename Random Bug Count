	// Get IDs from File 2
					var file2Ids = worksheet2
						.Column(columnName2) // TODO : id !=id of f1, take count of only status = "Fail"
						.CellsUsed()
						.Skip(1) // Skip header
						.Select(cell => cell.GetValue<string>())
						.Where(row =>
						{
							var statusCell = row.Cell(statusColumnName);
							return statusCell != null &&
								   statusCell.GetValue<string>().Equals(statusValue, StringComparison.OrdinalIgnoreCase); // Filter rows where Status = "Fail"
						})
						.Where(id => !string.IsNullOrWhiteSpace(id))
						.ToHashSet(StringComparer.OrdinalIgnoreCase); // Use HashSet for efficient matching
